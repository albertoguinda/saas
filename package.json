{
  "name": "saas-web-builder",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--trace-warnings next dev --turbo",
    "refresh-db": "tsx scripts/reset.ts && tsx scripts/seed.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix",
    "format": "prettier --write .",
    "seed": "tsx scripts/seed.ts",
    "translations:check": "tsx scripts/check-translations.ts",
    "i18n:check": "tsx scripts/i18n-check.ts",
    "export:zip": "next build && next export && zip -r out.zip out",
    "test": "node --test jest-runner.mjs",
    "check": "npm run lint && npm run format && npm test",
    "setup": "sh scripts/setup.sh",
    "check:legal": "tsx scripts/check-legal.ts",
    "prepare": "husky install",
    "lhci": "lhci autorun --collect.url=http://localhost:3000 --collect.numberOfRuns=1 --upload.target=temporary-public-storage",
    "ci:test": "start-server-and-test dev http://localhost:3000 npm run lhci",
    "backup": "tsx scripts/backup.ts",
    "backup:local": "tsx scripts/backup.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.842.0",
    "@heroui/alert": "^2.2.22",
    "@heroui/avatar": "^2.2.18",
    "@heroui/badge": "^2.2.14",
    "@heroui/button": "2.2.22",
    "@heroui/card": "^2.2.21",
    "@heroui/code": "2.2.16",
    "@heroui/dropdown": "^2.3.22",
    "@heroui/input": "2.4.22",
    "@heroui/kbd": "2.2.17",
    "@heroui/link": "2.2.19",
    "@heroui/listbox": "2.3.21",
    "@heroui/navbar": "2.2.20",
    "@heroui/ripple": "2.2.17",
    "@heroui/snippet": "2.2.23",
    "@heroui/switch": "2.2.20",
    "@heroui/system": "2.4.18",
    "@heroui/theme": "2.4.17",
    "@heroui/toast": "^2.0.12",
    "@hookform/resolvers": "^5.1.1",
    "@next-auth/mongodb-adapter": "^1.1.3",
    "@react-aria/visually-hidden": "3.8.24",
    "@stripe/stripe-js": "^7.4.0",
    "@upstash/redis": "^1.35.0",
    "atropos": "2.0.2",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "clamav.js": "^0.12.0",
    "cloudinary": "^2.7.0",
    "clsx": "2.1.1",
    "dotenv": "^17.0.0",
    "framer-motion": "11.13.1",
    "intl-messageformat": "10.7.16",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.525.0",
    "mongodb": "^5.9.2",
    "mongoose": "^8.16.1",
    "next": "15.3.1",
    "next-auth": "^4.24.11",
    "next-intl": "^4.3.4",
    "next-themes": "0.4.6",
    "react": "18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.59.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^2.1.0",
    "resend": "^4.6.0",
    "stripe": "^18.2.1",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.0",
    "@lhci/cli": "^0.13.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "20.5.7",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "8.31.1",
    "@typescript-eslint/parser": "8.31.1",
    "autoprefixer": "10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "16.0.0",
    "husky": "^9.0.11",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "^15.2.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "start-server-and-test": "^2.0.2",
    "tailwind-variants": "0.3.0",
    "tailwindcss": "3.4.16",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "5.6.3",
    "whatwg-fetch": "^3.6.20"
  }
}
